
@{
    ViewData["Title"] = "Login";
}

<form>
    <div class="form-group">
        <label for="UserName">Nazwa Użytkownika</label>
        <input type="text" class="form-control" id="UserName" aria-describedby="UserNameHelp" placeholder="Wprowadź nazwę użytkownika">
        <small id="UserNameHelp" class="form-text text-muted">Podaj nazwę użytkownika</small>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Hasło</label>
        <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Hasło">
    </div>

    <button type="submit" class="btn btn-primary">Wprowadź</button>
</form>

